list1=[]
n=int(input("enter the number of elements you need in the list: "))
if n<=0:
    print("enter a positive value")
else:
    print("enter the elements in the list: ")
    for i in range(0,n):
        elt=eval(input())
        list1.append(elt)
print("list1 =",list1)
x=1
for x in range(0,n):
    if list1[x]>0:
        print(list1[x],end =" ")
        x+=1
